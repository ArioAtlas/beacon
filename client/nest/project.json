{
  "name": "nest",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "client/nest/src",
  "projectType": "library",
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "dist/{projectRoot}",
        "currentVersionResolver": "git-tag"
      }
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/client/nest",
        "tsConfig": "client/nest/tsconfig.lib.json",
        "packageJson": "client/nest/package.json",
        "main": "client/nest/src/index.ts",
        "assets": ["client/nest/*.md"]
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    }
  }
}
